{
   "manifest_version": 2,
   "name": "Testing",
   "version": "0.1.0",
   "content_scripts": [
      {
         "matches": ["<all_urls>"],
         "js": ["content-script.js"]
      }
   ],
   "browser_action": {
      "default_popup": "popup.html",
      "default_title": "testing extension"
   },
   "permissions": ["activeTab"]
}
